<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DeviceCtl">
        <attr name="ctlIcon" format="reference"/>
        <attr name="ctlUnit" format="string"/>
    </declare-styleable>

    <declare-styleable name="RoundRect">
        <attr name="roundRectRadius" format="integer"/>
        <attr name="roundRectMode">
            <enum name="Circle" value="0"/>
            <enum name="RoundRect" value="1"/>
        </attr>
    </declare-styleable>


    <declare-styleable name="RunInfor">
        <attr name="runinforIcon" format="reference"/>
        <attr name="runinforName" format="string"/>
        <attr name="runinforUint" format="string"/>
        <attr name="runinforValue" format="string"/>
    </declare-styleable>

    <declare-styleable name="RoundProgressBar">
        <attr name="bgColor" format="color"/>
        <attr name="progressColor" format="color"/>
        <attr name="roundWidth" format="dimension"/>
        <attr name="maxProgress" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ProgramChart">
        <attr name="bgFrameColor" format="color"/>
        <attr name="blockColor" format="color"/>
        <attr name="runBlockColor" format="color"/>
        <attr name="lineColor" format="color"/>
        <attr name="paddingLeft" format="dimension"/>
        <attr name="paddingRight" format="dimension"/>
        <attr name="paddingTop" format="dimension"/>
        <attr name="paddingBottom" format="dimension"/>
        <attr name="blockWith" format="dimension"/>
        <attr name="blockHeight" format="dimension"/>
        <attr name="bgFrameRadius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="Commons">
        <attr name="icons" format="reference|color"/>
        <attr name="name" format="reference|string"/>
        <attr name="unit" format="reference|string"/>
        <attr name="value" format="reference|string"/>
        <attr name="txsize" format="dimension"/>
        <attr name="isShowBtn" format="boolean"/>
    </declare-styleable>
</resources>